{
  "name": "html-to-pdf",
  "version": "1.0.0",
  "description": "A NodeJS server to generate PDF from HTML.",
  "author": "David Robert-Ansart <david.robertansart@gmail.com>",
  "packageManager": "yarn@3.1.1",
  "license": "MIT",
  "main": "dist/app.js",
  "scripts": {
    "typescript": "tsc -p .",
    "build": "swc ./src -d dist",
    "start": "node dist/app.js",
    "start-dev": "yarn build && yarn start",
    "dev": "nodemon --watch ./src --ext ts --exec yarn start-dev "
  },
  "devDependencies": {
    "@swc/cli": "^0.1.55",
    "@swc/core": "^1.2.142",
    "@types/body-parser": "1.19.2",
    "@types/cors": "2.8.12",
    "@types/express": "4.17.13",
    "@types/fs-extra": "9.0.13",
    "@types/morgan": "1.9.3",
    "@types/node": "17.0.18",
    "@types/uuid": "8.3.4",
    "@typescript-eslint/eslint-plugin": "5.12.0",
    "@typescript-eslint/parser": "5.12.0",
    "eslint": "8.9.0",
    "eslint-config-prettier": "8.4.0",
    "eslint-import-resolver-node": "0.3.6",
    "eslint-import-resolver-typescript": "2.5.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-prettier": "4.0.0",
    "nodemon": "^2.0.15",
    "prettier": "2.5.1",
    "typescript": "4.5.5"
  },
  "dependencies": {
    "ajv": "^8.10.0",
    "ajv-formats": "^2.1.1",
    "body-parser": "^1.19.2",
    "cors": "^2.8.5",
    "express": "^4.17.3",
    "fs-extra": "^10.0.0",
    "helmet": "^5.0.2",
    "morgan": "^1.10.0",
    "puppeteer": "^13.3.2",
    "uuid": "^8.3.2",
    "winston": "^3.6.0"
  }
}
